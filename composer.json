{
    "name": "netro42/minify-html",
    "description": "Html minifier for CakePHP5",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "html",
        "compress",
        "minify",
        "cakephp",
        "cakephp5"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        },
        {
            "name": "Nitinai Supreeyaporn",
            "email": "nitinai.supreeyaporn@netro42.com",
            "homepage": "https://netro42.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "cakephp/cakephp": "^5.0",
        "cakephp/chronos": "^3.0",
        "wyrihaximus/html-compress": "^2.0 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.15 || ^9.3",
        "phpspec/prophecy": "^1.11",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Netro42\\MinifyHtml\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Netro42\\CakePHP\\Tests\\MinifyHtml\\View\\Helper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    }
}
